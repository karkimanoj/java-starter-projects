package com.javatpoint.server.main.Filtering;

import java.util.Arrays;

import org.springframework.web.bind.annotation.GetMapping;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;

@RestController
public class FilteringController 
{

	@RequestMapping("/filtering")
	public SomeBean retrieveSomeBean()
	{
		return new SomeBean("suman", "9867544567", "5000usd");
	}
	
	@RequestMapping("/filtering/listSomeBeans")
	public String getdata()
	{
		return Arrays.asList(new SomeBean("suman", "9867544567", "5000usd"), new SomeBean("suman", "9867544567", "5000usd"))
				(new SomeBean("suman", "9867544567", "5000usd"), new SomeBean("suman", "9867544567", "5000usd"));
	}
	

	
}
